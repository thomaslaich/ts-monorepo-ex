{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Nest.js",
  "extends": "./base.json",
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    "declarationMap": false,
    "sourceMap": true,
    "removeComments": true,
    "incremental": true,
    "strictBindCallApply": false
  }
}
